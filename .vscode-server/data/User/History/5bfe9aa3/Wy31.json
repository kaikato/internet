{
    "workbench.colorTheme": "Visual Studio Dark - C++",
    "git.autofetch": true,
    "gopls": {
        "experimentalWorkspaceModule": true,

        "build.env": {
            "GOOS": "linux"
        },
    },
    "[go]": {
        "editor.codeActionsOnSave": {
